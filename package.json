{
  "name": "primitive-ui",
  "version": "1.5.2",
  "author": "Tania Rascia <tania@taniarascia.com>",
  "description": "A minimalist front-end design toolkit built with Sass for developing responsive, browser-consistent web apps.",
  "homepage": "https://taniarascia.github.io/primitive",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/taniarascia/primitive.git"
  },
  "dependencies": {
    "node-sass": "^4.13.1",
    "prettier": "^1.19.1"
  },
  "scripts": {
    "sass": "node-sass ./scss/ -o ./css/ -w -r --source-map-embed --source-map-contents --output-style expanded | node-sass ./scss/main.scss ./css/main.min.css -w --output-style compressed"
  },
  "keywords": [
    "sass",
    "setup",
    "boilerplate",
    "framework",
    "ui",
    "css framework",
    "front end",
    "design"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/taniarascia/primitive/issues"
  }
}
